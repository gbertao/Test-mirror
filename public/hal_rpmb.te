# HwBinder IPC from client to server, and callbacks
binder_call(hal_rpmb_client, hal_rpmb_server)
binder_call(hal_rpmb_server, hal_rpmb_client)

hal_attribute_service(hal_rpmb, hal_rpmb_service)

binder_use(hal_rpmb_server)
binder_use(hal_rpmb_client)

allow hal_rpmb_client hal_rpmb_service:service_manager find;
