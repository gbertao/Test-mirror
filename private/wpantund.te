typeattribute wpantund coredomain;

init_daemon_domain(wpantund)

# Audit access the MAC address and link information for potential removal (b/192350019)
allow wpantund self:netlink_route_socket { bind nlmsg_readpriv nlmsg_getneigh };
userdebug_or_eng(`
  auditallow wpantund self:netlink_route_socket { bind nlmsg_readpriv nlmsg_getneigh };
')
