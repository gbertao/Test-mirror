typeattribute crash_dump coredomain;

allow crash_dump {
  domain
  -bpfloader
  -crash_dump
  -init
  -kernel
  -keystore
  -llkd
  -logd
  -ueventd
  -vendor_init
  -vold
}:process { ptrace signal sigchld sigstop sigkill };
userdebug_or_eng(`
  allow crash_dump { llkd logd }:process { ptrace signal sigchld sigstop sigkill };
')

neverallow crash_dump self:process ptrace;
