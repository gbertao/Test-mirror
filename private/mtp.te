typeattribute mtp coredomain;

init_daemon_domain(mtp)

# Audit access the MAC address and link information for potential removal (b/192350019)
allow mtp self:netlink_route_socket { bind nlmsg_readpriv nlmsg_getneigh };
userdebug_or_eng(`
  auditallow mtp self:netlink_route_socket { bind nlmsg_readpriv nlmsg_getneigh };
')
