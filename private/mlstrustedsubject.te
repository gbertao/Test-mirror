# MLS override can't be used to access private app data.
neverallow {
  mlstrustedsubject
  -installd
  -iorap_prefetcherd
userdebug_or_eng(`
  -su
')
} { app_data_file privapp_data_file }:file ~{ read write map getattr ioctl lock append };

neverallow {
  mlstrustedsubject
  -installd
  -iorap_prefetcherd
} { app_data_file privapp_data_file }:dir ~{ read getattr search };
